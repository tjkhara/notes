   "source": [
    "import os\n",
    "for root, dirs, files in os.walk(\".\", topdown=False):\n",
    "   for name in files:\n",
    "      print(os.path.join(root, name))\n",
    "      full_name = os.path.join(root, name)\n",
    "      with open(full_name) as myfile:\n",
    "          lines=myfile.read()\n",
    "          print(lines)"
